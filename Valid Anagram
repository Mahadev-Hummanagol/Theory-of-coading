def is_anagram(s: str, t: str) -> bool:
    # If lengths differ, they cannot be anagrams
    if len(s) != len(t):
        return False

    freq = {}  # HashMap: character -> count

    # Count frequency of each character in s
    for ch in s:
        freq[ch] = freq.get(ch, 0) + 1

    # Reduce count using t
    for ch in t:
        if ch not in freq:
            return False  # character in t not present in s
        freq[ch] -= 1
        if freq[ch] < 0:
            return False  # more occurrences in t than in s

    return True

